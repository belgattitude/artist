{
  "name": "artist-strapi-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "browser": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build-disabled": "tsup",
    "lint": "cross-env TIMING=1 eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts,.mdx --cache --cache-location ../../.cache/api.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "codegen": "dotenv-flow -- tsx ./scripts/generate-openapi-types.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "7.2.0",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/typescript-estree": "8.34.1",
    "@types/fs-extra": "11.0.4",
    "@types/node": "24.0.3",
    "cross-env": "7.0.3",
    "dotenv-flow-cli": "1.1.1",
    "esbuild": "0.25.5",
    "eslint": "8.57.1",
    "openapi-typescript": "7.8.0",
    "picocolors": "1.1.1",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "rollup": "4.44.0",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "valibot": "1.1.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
