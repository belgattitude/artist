{
  "name": "artist-strapi-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "lint": "cross-env TIMING=1 eslint .. --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts,.mdx --cache --cache-location ../../.cache/api.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "codegen-api": "dotenv-flow -- node ./scripts/codegen.mjs"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "1.26.0",
    "cross-env": "7.0.3",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.36.0",
    "fs-extra": "11.1.1",
    "openapi-typescript": "6.2.0",
    "picocolors": "1.0.0",
    "prettier": "2.8.6",
    "rimraf": "4.4.1",
    "tsup": "6.7.0",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "0.29.7"
  },
  "dependencies": {
    "@types/fs-extra": "11.0.1"
  }
}
