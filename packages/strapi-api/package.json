{
  "name": "artist-strapi-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "browser": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build-disabled": "tsup",
    "lint": "cross-env TIMING=1 eslint .. --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts,.mdx --cache --cache-location ../../.cache/api.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "codegen": "dotenv-flow -- node ./scripts/generate-openapi-types.mjs"
  },
  "dependencies": {
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "2.7.0",
    "@types/fs-extra": "11.0.2",
    "@types/node": "20.8.3",
    "cross-env": "7.0.3",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.51.0",
    "fs-extra": "11.1.1",
    "openapi-typescript": "6.7.0",
    "picocolors": "1.0.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  }
}
