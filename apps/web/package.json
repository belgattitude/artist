{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-no-tls": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 next build --no-lint",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts,.mdx --cache --cache-location ../../.cache/web.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "codegen-schema": "dotenv-flow -- pnpm graphql-codegen --config codegen.schema-ast.ts",
    "codegen": "dotenv-flow -- pnpm graphql-codegen --config codegen.ts",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@httpx/exception": "1.7.1",
    "@next/font": "13.1.7-canary.8",
