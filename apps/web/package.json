    "@tanstack/react-query": "4.24.4",
    "@vanilla-extract/dynamic": "2.0.3",
    "@vercel/og": "0.1.0",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "graphql-request": "5.1.0",
    "next": "13.1.7-canary.8",
    "next-secure-headers": "2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.5",
    "remark": "14.0.2",
    "remark-gfm": "3.0.1",
    "remark-html": "15.0.2",
    "rooks": "7.4.3",
    "sharp": "0.31.3",
    "tailwind-merge": "1.9.1",
    "theme-ui": "0.15.5",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "1.20.2",
