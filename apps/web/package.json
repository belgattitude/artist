{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-no-tls": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_LINT=1 next build --no-lint",
    "start": "next start",
    "lint": "cross-env TIMING=1 eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts,.mdx --cache --cache-location ../../.cache/web.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "codegen-schema": "dotenv-flow -- pnpm graphql-codegen --config codegen.schema-ast.ts",
    "codegen": "dotenv-flow -- pnpm graphql-codegen --config codegen.ts",
    "favpie": "npx favpie ./public/favicon.ico ./public/favicon --app-name 'Reflective Resonance' --short-name 'Reflective Resonance'",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
